{
  "$comment": "Strategy presets - each maps all harnesses to a method_name. Missing harness.method entries in methods.json trigger fallback.",
  "presets": {
    "mise_binary": {
      "strategy": "mise_binary",
      "description": "Prefer mise-managed prebuilt binaries (mise plugins / github backend) when available",
      "priority": 1,
      "methods": {
        "crush": "mise_binary",
        "kilocode": "mise_binary",
        "opencode": "mise_binary",
        "claude": "mise_binary",
        "grok": "mise_binary",
        "gemini": "mise_binary",
        "cline": "mise_binary",
        "continue": "mise_binary",
        "codex": "mise_binary",
        "amp": "mise_binary",
        "interpreter": "mise_binary",
        "openhands": "mise_binary",
        "aider": "mise_binary",
        "mistral": "mise_binary",
        "kimi": "mise_binary",
        "droid": "mise_binary",
        "goose": "mise_binary",
        "roo": "mise_binary"
      },
      "fallback_strategy": "mise_language"
    },
    "mise_language": {
      "strategy": "mise_language",
